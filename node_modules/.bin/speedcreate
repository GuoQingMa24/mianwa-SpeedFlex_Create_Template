#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/mianwa-SpeedFlex_Create_Template/node_modules/.pnpm/speedcreate@1.0.0_@types+node@22.13.9/node_modules/speedcreate/src/node_modules:/mnt/e/mianwa-SpeedFlex_Create_Template/node_modules/.pnpm/speedcreate@1.0.0_@types+node@22.13.9/node_modules/speedcreate/node_modules:/mnt/e/mianwa-SpeedFlex_Create_Template/node_modules/.pnpm/speedcreate@1.0.0_@types+node@22.13.9/node_modules:/mnt/e/mianwa-SpeedFlex_Create_Template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/mianwa-SpeedFlex_Create_Template/node_modules/.pnpm/speedcreate@1.0.0_@types+node@22.13.9/node_modules/speedcreate/src/node_modules:/mnt/e/mianwa-SpeedFlex_Create_Template/node_modules/.pnpm/speedcreate@1.0.0_@types+node@22.13.9/node_modules/speedcreate/node_modules:/mnt/e/mianwa-SpeedFlex_Create_Template/node_modules/.pnpm/speedcreate@1.0.0_@types+node@22.13.9/node_modules:/mnt/e/mianwa-SpeedFlex_Create_Template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../speedcreate/src/index.ts" "$@"
else
  exec node  "$basedir/../speedcreate/src/index.ts" "$@"
fi
